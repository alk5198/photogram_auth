<div class="page-header">
  <h3>My Likes</h3>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>

        <th>Caption</th>

        <th>Image</th>




      </tr>

      <% @my_likes.each do |like| %>

      <tr>

        <td><%= like.photo.caption %></td>

        <td> <img src="<%= like.photo.image %>" width="400"></td>



      </tr>
      <% end %>
    </table>
  </div>
</div>
